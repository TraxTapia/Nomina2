@{
    Layout = "_Layout";
    var _ListCategory = new List<SelectListItem>();
    if (ViewBag.Select != null)
    {
        _ListCategory = (List<SelectListItem>)ViewBag.Select;
    }
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Nomina</a></li>
                    <li class="breadcrumb-item active" aria-current="page">
                        Nuevo Empleado
                    </li>
                </ol>
            </nav>
        </div>
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="container">
                        <div class="row mt-3">
                            <div class="col-md-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h4 class="text-center">Nuevo Empleado</h4>
                                    </div>
                                    <div class="card-body">
                                        <div class="container">
                                            <form id ="frmEmployerNew">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="txtNombre">Nombre</label>
                                                        <input type="email" class="form-control" id="txtNombre" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="txtApellidoPaterno">Apellido Paterno</label>
                                                        <input type="email" class="form-control" id="txtApellidoPaterno" />
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Apellido Materno</label>
                                                        <input type="email" class="form-control" id="txtApellidoMaterno" />
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Código Empresa</label>
                                                        <input type="email" class="form-control" id="txtNombre" />
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="form-group">
                                                        <label for="exampleInputEmail1">Posición</label>
                                                        <input type="email" class="form-control" id="txtApellidoPaterno" />
                                                    </div>
                                                </div>
                                                <div class="col-md-3">
                                                    <div class="form-group">
                                                        <label for="exampleFormControlSelect1">Estatus</label>
                                                        <select class="form-control form-control-md">
                                                            <option selected disabled>--Selecciona--</option>
                                                            <option value="A">Activo</option>
                                                            <option value="B">Baja</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <img style="width: 150px !important"
                                                         class="rounded-circle w-100"
                                                         src="data:image/jpeg;base64,"
                                                         alt="..." />
                                                </div>
                                            </div>                                            
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <nav>
                                                        <div class="nav nav-tabs" id="nav-tab" role="tablist">
                                                            <a class="nav-item nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">Ubicación</a>
                                                            <a class="nav-item nav-link" id="nav-profile-tab" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">Fechas y Sueldos</a>
                                                            <a class="nav-item nav-link" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Fiscales y Personales</a>
                                                        </div>
                                                    </nav>
                                                    <div class="tab-content" id="nav-tabContent">
                                                        <div class="tab-pane fade " id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
                                                            <partial name="_UbicacionVP.cshtml" />
                                                        </div>

                                                        <div class="tab-pane fade" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
                                                            <partial name="_FiscalesPersonalesVP.cshtml" />

                                                        </div>
                                                        <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
                                                            <partial name="_FechasSueldosVP.cshtml" />
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row d-flex justify-content-end align-items-center">
                                              @*<button onclick="$('#frmEmployerNew').submit()" type="button" class="btn btn-outline-success">Guardar<span> <i class="bx bx-save"></i></span></button>*@
                                              <button onclick="$('#frmEmployerNew').submit()" type="button" class="btn btn-outline-success">Guardar</button>
                                            </div>
                                            </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {

<script src="~/js/Empleados/Index.js"></script>
    }